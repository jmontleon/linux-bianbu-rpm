         >;
     };
 
@@ -953,7 +953,7 @@ K1X_PADCONF(GPIO_113,    MUX_MODE3, (EDGE_NONE | PULL_DOWN | PAD_1V8_DS2))  /* P
         >;
     };
 
-    pinctrl_i2c2_1: i2c2_1_grp {
+    pinctrl_i2c2_2: i2c2_2_grp {
         pinctrl-single,pins =<
             K1X_PADCONF(GPIO_68,    MUX_MODE3, (EDGE_NONE | PULL_DOWN | PAD_1V8_DS2))   /* i2c2_scl */
