+			#clock-cells = <1>;
 			status = "okay";
 		};
 
 		reset: reset-controller@d4050000 {
-			ranges;
 			compatible = "spacemit,k1x-reset";
 			reg = <0x0 0xd4050000 0x0 0x209c>,
 				<0x0 0xd4282800 0x0 0x400>,
