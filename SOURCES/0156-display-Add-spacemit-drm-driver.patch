+static int spacemit_dsi_phy_attach(struct spacemit_dsi *dsi)
+{
+	struct device *dev;
+
+	dev = spacemit_disp_pipe_get_output(&dsi->dev);
+	if (!dev)
+		return -ENODEV;
+
+	dsi->ctx.phy = dev_get_drvdata(dev);
+	if (!dsi->ctx.phy) {
+		DRM_ERROR("dsi attach phy failed\n");
+		return -EINVAL;
+	}
+
+	return 0;
+}
