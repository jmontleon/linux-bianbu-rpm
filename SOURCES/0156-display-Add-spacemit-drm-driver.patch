+	ROTATE_COMMON = BIT(0),		/* supports rotation on raw and afbc 0/180, x and y */
+	ROTATE_RAW_90_270 = BIT(1),	/* supports rotation on raw 90/270 */
+	ROTATE_AFBC_90_270 = BIT(2),	/* supports rotation on afbc 90/270 */
+};
