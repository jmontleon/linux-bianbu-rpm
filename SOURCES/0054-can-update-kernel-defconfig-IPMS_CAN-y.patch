 # CONFIG_IPV6 is not set
 CONFIG_NETLINK_DIAG=y
+CONFIG_CAN=y
 CONFIG_FAILOVER=y
 # CONFIG_ETHTOOL_NETLINK is not set
 CONFIG_PCI=y
@@ -103,6 +104,7 @@ CONFIG_DWMAC_SPACEMIT=y
