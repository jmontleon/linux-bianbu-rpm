 	/* Create <group>/severity. */
-	debugfs_create_u32("severity", 0600, group->dentry,
+	/*debugfs_create_u32("severity", 0600, group->dentry,
 				    &group->severity);
 	if (IS_ERR_OR_NULL(dentry)) {
 		pr_err("MVX: Failed to create '%s/severity' value.\n",
 		       name);
 		ret = -ENOMEM;
 		goto error;
-	}
+	}*/
 
 	/* Create <group>/drain. */
-	debugfs_create_file("drain", 0600, group->dentry, NULL,
+	/*debugfs_create_file("drain", 0600, group->dentry, NULL,
 				     &group_drain_fops);
 	if (IS_ERR_OR_NULL(dentry)) {
 		pr_err("MVX: Failed to create '%s/severity' value.\n",
 		       name);
 		ret = -ENOMEM;
 		goto error;
-	}
+	}*/
 
 	return 0;
 
